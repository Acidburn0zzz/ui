{{#if session.isAuthenticated}}
<div class="pure-g">
  <div class="pure-u-21-24">
    {{pipeline-workflow jobs=model.jobs}}
  </div>
  <div class="pure-u-3-24 right">
    {{pipeline-start startBuild=(action "startMainBuild")}}
  </div>
</div>
{{else}}
{{pipeline-workflow jobs=model.jobs}}
{{/if}}


<div class="pure-g">
  <div class="pure-u-1 pure-u-md-3-4">
    {{pipeline-events-list events=model.pipeline.events}}
  </div>
  <div class="pure-u-1 pure-u-md-1-4">
    {{pipeline-pr-list pullRequests=model.pullRequests}}
  </div>
</div>


{{outlet}}